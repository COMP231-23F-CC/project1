#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bowendai/Documents/project1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+rollup@3.29.4/node_modules/rollup/dist/bin/node_modules:/Users/bowendai/Documents/project1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+rollup@3.29.4/node_modules/rollup/dist/node_modules:/Users/bowendai/Documents/project1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+rollup@3.29.4/node_modules/rollup/node_modules:/Users/bowendai/Documents/project1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+rollup@3.29.4/node_modules:/Users/bowendai/Documents/project1/guest-side/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bowendai/Documents/project1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+rollup@3.29.4/node_modules/rollup/dist/bin/node_modules:/Users/bowendai/Documents/project1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+rollup@3.29.4/node_modules/rollup/dist/node_modules:/Users/bowendai/Documents/project1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+rollup@3.29.4/node_modules/rollup/node_modules:/Users/bowendai/Documents/project1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+rollup@3.29.4/node_modules:/Users/bowendai/Documents/project1/guest-side/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmjs.org+rollup@3.29.4/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../registry.npmjs.org+rollup@3.29.4/node_modules/rollup/dist/bin/rollup" "$@"
fi
