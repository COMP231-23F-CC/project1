#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hover/git/cc/ccjinshu/COMP231/ccbnb_v1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules/loose-envify/node_modules:/Users/hover/git/cc/ccjinshu/COMP231/ccbnb_v1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules:/Users/hover/git/cc/ccjinshu/COMP231/ccbnb_v1/guest-side/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hover/git/cc/ccjinshu/COMP231/ccbnb_v1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules/loose-envify/node_modules:/Users/hover/git/cc/ccjinshu/COMP231/ccbnb_v1/guest-side/client/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules:/Users/hover/git/cc/ccjinshu/COMP231/ccbnb_v1/guest-side/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
